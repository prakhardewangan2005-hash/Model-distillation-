{
  "name": "MODEL DISTILLATION LAB (CPU)",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "features": {},
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance"
      ],
      "settings": {
        "python.defaultInterpreterPath": "/usr/local/bin/python",
        "python.analysis.typeCheckingMode": "basic"
      }
    }
  },
  "postCreateCommand": "python -m venv .venv && . .venv/bin/activate && pip install -U pip && pip install -e .",
  "remoteUser": "vscode"
}
